<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:gravity="center"
    android:orientation="vertical"
    android:padding="50dp">


    <TextView
        android:id="@+id/center_head_dashboard_tv_meal_entry"
        android:layout_width="match_parent"
        android:layout_height="120dp"
        android:background="@drawable/black_round_bg"
        android:gravity="center"
        android:text="@string/center_head_dashboard_meal_entry"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="25sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/center_head_dashboard_tv_daily_report"
        android:layout_width="match_parent"
        android:layout_height="120dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/black_round_bg"
        android:gravity="center"
        android:text="@string/center_head_dashboard_daily_meal_report"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="25sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/center_head_dashboard_tv_monthly_report"
        android:layout_width="match_parent"
        android:layout_height="120dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/black_round_bg"
        android:gravity="center"
        android:text="@string/center_head_dashboard_monthly_meal_report"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="25sp"
        android:textStyle="bold" />

</LinearLayout>